    "devDependencies": {
        "@nexcodepl/endpoint-types": "^1.0.0",
        "@types/react": "^18.0.31",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "eslint": "^8.37.0",
        "prettier": "^2.8.7",
        "typescript": "^5.0.3"
    },
    "files": [
        "dist/**/*"
    ]
}
